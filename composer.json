{
    "name": "orklah/psalm-strict-types",
    "description": "Adds strict_types declaration when the file is provably safe",
    "type": "psalm-plugin",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "authors": [
        {
            "name": "orklah"
        }
    ],
    "extra": {
        "psalm" : {
            "pluginClass": "Orklah\\StrictTypes\\Plugin"
        }
    },
    "require": {
        "php": "^7.3|^8.0",
        "vimeo/psalm": "^4.4.0"
    },
    "autoload": {
        "psr-4": {
            "Orklah\\StrictTypes\\": ["./src"],
            "Orklah\\StrictTypes\\Tests\\": ["./tests"]
        }
    },
    "require-dev": {
        "nikic/php-parser": "^4.0",
        "phpunit/phpunit": "^9.5",
        "psalm/plugin-phpunit": "^0.15.0"
    }
}
